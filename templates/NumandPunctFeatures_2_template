# NumandPunctFeatures context = 2
# word
U00:%x[-2,0]
U01:%x[-1,0]
U02:%x[0,0]
U03:%x[1,0]
# isalpha
U04:%x[-2,1]
U05:%x[-1,1]
U06:%x[0,1]
U07:%x[1,1]
# hasNumbers
U08:%x[-2,2]
U09:%x[-1,2]
U10:%x[0,2]
U11:%x[1,2]
# allNum
U12:%x[-2,3]
U13:%x[-1,3]
U14:%x[0,3]
U15:%x[1,3]
# endsWithPunctuation
U16:%x[-2,4]
U17:%x[-1,4]
U18:%x[0,4]
U19:%x[1,4]
# startsWithPunctuation
U20:%x[-2,5]
U21:%x[-1,5]
U22:%x[0,5]
U23:%x[1,5]
