# isAlpha
U00:%x[-8,0]
U01:%x[-7,0]
U02:%x[-6,0]
U03:%x[-5,0]
U04:%x[-4,0]
U05:%x[-3,0]
U06:%x[-2,0]
U07:%x[-1,0]
U08:%x[0,0]
U09:%x[1,0]
U10:%x[2,0]
U11:%x[3,0]
U12:%x[4,0]
U13:%x[5,0]
U14:%x[6,0]
U15:%x[7,0]

# isNumeric
U16:%x[-4,1]
U17:%x[-3,1]
U18:%x[-2,1]
U19:%x[-1,1]
U20:%x[0,1]
U21:%x[1,1]
U22:%x[2,1]
U23:%x[3,1]

# isPunct
U24:%x[-4,2]
U25:%x[-3,2]
U26:%x[-2,2]
U27:%x[-1,2]
U28:%x[0,2]
U29:%x[1,2]
U30:%x[2,2]
U31:%x[3,2]

# isUpper
U32:%x[-4,3]
U33:%x[-3,3]
U34:%x[-2,3]
U35:%x[-1,3]
U36:%x[0,3]
U37:%x[1,3]
U38:%x[2,3]
U39:%x[3,3]

# isLower
U40:%x[-4,4]
U41:%x[-3,4]
U42:%x[-2,4]
U43:%x[-1,4]
U44:%x[0,4]
U45:%x[1,4]
U46:%x[2,4]
U47:%x[3,4]

# is<sp>
U48:%x[-4,5]
U49:%x[-3,5]
U50:%x[-2,5]
U51:%x[-1,5]
U52:%x[0,5]
U53:%x[1,5]
U54:%x[2,5]
U55:%x[3,5]

# char
U56:%x[-4,6]
U57:%x[-3,6]
U58:%x[-2,6]
U59:%x[-1,6]
U60:%x[0,6]
U61:%x[1,6]
U62:%x[2,6]
U63:%x[3,6]

